@model UserOwnsData.Services.EmbeddedReportViewModel;

<style>
  .mb-3, .my-3 { margin-bottom: 0 !important; }
</style>

<div id="embed-container" style="height:800px;"></div>

@section Scripts {
  <script src="~/lib/powerbi-client/powerbi.js"></script>
  <script>
    var viewModel = {
      reportId: "@Model.Id",
      embedUrl: "@Model.EmbedUrl",
      token: "@Model.Token"
    };
  </script>
  <script src="~/js/embed.js"></script>
}